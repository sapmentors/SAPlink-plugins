	METHOD CHECKEXISTS.



	  exists = abap_true.

	  DATA: config_id TYPE WDY_CONFIG_ID.
	  config_id = objname.
	  TRANSLATE config_id TO UPPER CASE.

	  SELECT SINGLE config_id FROM wdy_config_data INTO config_id
	    WHERE config_id = config_id.
	  IF sy-subrc NE 0.
	    CLEAR exists.
	  ENDIF.


