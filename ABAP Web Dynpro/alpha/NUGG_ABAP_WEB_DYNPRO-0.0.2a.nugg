	METHOD DELETEOBJECT.
	  DATA: config_id TYPE wdy_config_id.
	  config_id = objname.
	  TRANSLATE config_id TO UPPER CASE.
	  DELETE FROM wdy_config_data WHERE config_id = config_id.
	  DELETE FROM wdy_config_datt WHERE config_id = config_id.

	  COMMIT WORK.

	ENDMETHOD.</source>
